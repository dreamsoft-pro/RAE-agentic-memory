{
  "mcpServers": {
    "rae-memory": {
      "command": ".venv/bin/rae-mcp-server",
      "env": {
        "RAE_API_URL": "http://localhost:8000",
        "RAE_API_KEY": "dev-key",
        "RAE_PROJECT_ID": "RAE-agentic-memory",
        "RAE_TENANT_ID": "default-tenant"
      }
    },
    "gemini": {
      "command": ".venv/bin/gemini-mcp-server",
      "env": {
        "RAE_API_URL": "http://localhost:8000",
        "RAE_API_KEY": "dev-key",
        "RAE_PROJECT_ID": "RAE-agentic-memory",
        "RAE_TENANT_ID": "default-tenant"
      }
    }
  }
}
