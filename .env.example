# Example environment variables
# Note: Do not use quotes around values in .env files

MEMORY_API_URL=http://localhost:8000
REDIS_URL=redis://localhost:6379/0

# --- Vector Store Configuration ---
# Set the backend to 'qdrant' or 'pgvector'
RAE_VECTOR_BACKEND=qdrant

# --- LLM Provider Configuration ---
# Set the backend to 'gemini', 'openai', or 'ollama'
RAE_LLM_BACKEND=ollama
# Set the default model for the chosen backend
RAE_LLM_MODEL_DEFAULT=llama3

# --- API Keys ---
# Replace with your actual API keys (without quotes!)
GEMINI_API_KEY=your-gemini-api-key
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# --- Service URLs ---
OLLAMA_API_URL=http://localhost:11434

# --- Celery Configuration ---
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# --- Memory Decay Configuration ---
MEMORY_DECAY_RATE=0.99

